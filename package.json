{
  "name": "rb-tools-notifications",
  "version": "1.0.0",
  "description": "Components providing data for Monitoring Consumers",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "cdk": "cdk",
    "deploy": "npm run build && npm run synth && npm run cdk deploy --require-approval=never",
    "format": "prettier . --write",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix && npm run format",
    "test": "jest --coverage",
    "synth": "npm run cdk synth --require-approval=never && sh scripts/clean_templates.sh",
    "_comment": "install this first npm install -g npm-check-updates",
    "update-deps": "ncu --reject eslint,eslint-config-prettier,eslint-config-standard-with-typescript,eslint-plugin-import,eslint-plugin-n,eslint-plugin-prettier,eslint-plugin-promise,@typescript-eslint/eslint-plugin,@typescript-eslint/parser,prettier,prettier-plugin-multiline-arrays -u && npm install",
    "zip": "ZIP_FILE=$PWD/package.zip && pwd && cd dist && zip -rq -9 $ZIP_FILE ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@aws-lambda-powertools/logger": "^2.14.0",
    "@aws-lambda-powertools/metrics": "^2.14.0",
    "@aws-lambda-powertools/tracer": "^2.14.0",
    "@aws-sdk/client-dynamodb": "3.751.0",
    "@aws-sdk/client-sns": "^3.750.0",
    "@aws-sdk/lib-dynamodb": "3.751.0",
    "@aws-sdk/types": "3.734.0",
    "@jest/types": "29.6.3",
    "@tsconfig/node18": "18.2.4",
    "@types/aws-lambda": "8.10.147",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "29.5.14",
    "@types/minimist": "1.2.5",
    "@types/node": "22.13.4",
    "@types/node-fetch": "2.6.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "aws-cdk": "2.1000.2",
    "aws-cdk-lib": "^2.179.0",
    "constructs": "10.4.2",
    "esbuild": "^0.25.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "~9.1.0",
    "eslint-config-standard-with-typescript": "37.0.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-n": "16.0.1",
    "eslint-plugin-prettier": "~5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "fs-extra": "^11.3.0",
    "jest": "29.7.0",
    "nock": "14.0.1",
    "prettier": "3.2.5",
    "prettier-plugin-multiline-arrays": "~3.0.4",
    "source-map-support": "0.5.21",
    "speculate": "5.0.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.24.1"
  }
}
